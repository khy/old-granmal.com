GET     /               controllers.core.IndexController.index
GET     /masthead       controllers.core.IndexController.masthead
GET     /apps           controllers.core.IndexController.apps

GET     /menu           controllers.core.MenuController.main

GET     /sign-in        controllers.core.SessionController.form
POST    /sign-in        controllers.core.SessionController.create
GET     /sign-out       controllers.core.SessionController.delete

GET     /sign-up        controllers.core.AccountController.form
POST    /sign-up        controllers.core.AccountController.create

GET     /oauth/useless  controllers.core.OAuthController.useless(code: String)

GET     /haikunst         controllers.haikunst.HaikunstController.index
GET     /haikunst/new     controllers.haikunst.HaikunstController.form
POST    /haikunst/haikus  controllers.haikunst.HaikunstController.create
GET     /haikunst/menu    controllers.haikunst.HaikunstController.menu
GET     /haikunst/:handle controllers.haikunst.HaikunstController.byUser(handle: String)

GET     /assets/*file   controllers.Assets.at(path="/public", file)
