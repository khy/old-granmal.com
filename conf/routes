GET     /               controllers.CoreController.index
GET     /menu           controllers.CoreController.menu

GET     /sign-in        controllers.SessionController.form
POST    /sign-in        controllers.SessionController.create
GET     /sign-out       controllers.SessionController.delete

GET     /oauth/useless  controllers.OAuthController.useless(code: String)

GET     /assets/*file   controllers.Assets.at(path="/public", file)
