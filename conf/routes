GET     /               controllers.core.IndexController.index

GET     /menu           controllers.core.MenuController.main

GET     /sign-in        controllers.core.SessionController.form
POST    /sign-in        controllers.core.SessionController.create
GET     /sign-out       controllers.core.SessionController.delete

GET     /sign-up        controllers.core.AccountController.form
POST    /sign-up        controllers.core.AccountController.create

GET     /oauth/useless  controllers.core.OAuthController.useless(code: String)

GET     /haikunst             controllers.haikunst.AppController.index
GET     /haikunst/haikus/new  controllers.haikunst.AppController.form
POST    /haikunst/haikus      controllers.haikunst.AppController.create
GET     /haikunst/menu        controllers.haikunst.AppController.menu

GET     /assets/*file   controllers.Assets.at(path="/public", file)
